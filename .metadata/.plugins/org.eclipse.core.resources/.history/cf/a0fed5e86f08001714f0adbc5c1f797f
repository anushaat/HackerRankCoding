
public class FibonacciDemo {
	static int n1 = 0, n2 = 1, n3=0;
	
	public static int Fibonacci(int n){
		
		if(n <= 0){
			return 0;
		}
		else{
				/*n3 = n1 + n2;
				n1 = n2;
				n2 = n3;*/
			
				n1 = n1 + n2;
				System.out.print(n1+" ");
				Fibonacci(n-1);
			}
			return n1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print(n1+" "+n2+" ");
		Fibonacci(10);	
	}

}
